<Multi_key> <f> <S>               : "ſ"   U017f # LATIN SMALL LETTER LONG S
<Multi_key> <period> <f> <s> : "ẛ"   U1e9b # LATIN SMALL LETTER LONG S WITH DOT ABOVE

<Multi_key> <W> <W>: "Ƿ" U01F7 # Wynn
